{
  "name": "pushpak1300.github.io",
  "version": "1.0.0",
  "description": "Portfolio Website built from scratch using Bootstrap4.",
  "main": "all.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "dev": "npm run development",
    "serve": "http-server",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production && purgecss --css build/all.css build/font.css --content index.html -o build/",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "critical": "^1.3.9",
    "cross-env": "^7.0",
    "laravel-mix": "^5.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pushpak1300/pushpak1300.github.io.git"
  },
  "author": "Pushpak Chhajed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pushpak1300/pushpak1300.github.io/issues"
  },
  "homepage": "https://github.com/pushpak1300/pushpak1300.github.io#readme",
  "dependencies": {
    "alpinejs": "^2.3.0",
    "aos": "^2.3.4",
    "bootstrap": "^4.4.1",
    "http-server": "^0.12.3",
    "jquery": "^3.5.0",
    "moment": "^2.24.0",
    "popper.js": "^1.14.3",
    "purgecss": "^2.1.2"
  }
}
